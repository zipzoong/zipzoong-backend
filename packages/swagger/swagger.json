        "responses": {
          "200": {
            "description": "사용자 권한이 부여된 토큰",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/IAuthentication.IResponse.ISignIn"
                }
              }
            },
            "x-nestia-encrypted": false
